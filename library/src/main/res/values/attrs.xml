<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HintedSpinner">
        <attr name="withDivider" format="boolean" />
        <attr name="dividerTint" format="color" />
        <attr name="arrowDrawable" format="reference" />
        <attr name="arrowTint" format="color" />
        <attr name="hint" format="string" />
        <attr name="hintTextAppearance" format="reference" />
        <attr name="popupBackground" format="color" />
        <attr name="popupMode" format="enum">
            <enum name="dialog" value="0" />
            <enum name="dropdown" value="1" />
        </attr>
    </declare-styleable>
</resources>